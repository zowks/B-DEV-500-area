{
  "name": "area-backend",
  "version": "0.0.1",
  "description": "AREA's backend source code.",
  "contributors": [
    {
      "name": "DURAND Malo",
      "email": "malo.durand@epitech.eu",
      "url": "https://github.com/Ximaz"
    },
    {
      "name": "RIGOMONT Elouan",
      "email": "elouan.rigomont@epitech.eu"
    }
  ],
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:dev": "nest build && npm run share-types",
    "build": "nest build",
    "format": "npm run format:write",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:migrate": "npx --env-file=.env -y prisma migrate dev",
    "prisma:generate": "npx -y prisma generate",
    "prisma:deploy": "npx --env-file=.env -y prisma migrate deploy",
    "start": "npm run build:dev && nest start",
    "start:dev": "npm run build:dev && nest --env-file=.env start --watch",
    "start:debug": "npm run build:dev && nest start --debug --watch",
    "start:prod": "node dist/main",
    "container": "npx prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:cov": "node node_modules/.bin/jest --runInBand --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "share-types": "bash ./share-types.bash"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": ".",
    "testRegex": ".(service|transformers|action|reaction)\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(service|transformers|action|reaction).ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.20.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.7",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "cron": "^3.1.7",
    "express-session": "^1.18.0",
    "helmet": "^8.0.0",
    "jose": "^5.9.3",
    "passport-strategy": "^1.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "remove": "^0.1.5",
    "rxjs": "^7.8.1",
    "swagger-themes": "^1.4.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/passport": "^1.0.16",
    "@types/passport-strategy": "^0.2.38",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-markdown": "^2.3.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
