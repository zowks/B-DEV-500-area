    "@rn-primitives/slot": "~1.0.3",
    "@rn-primitives/tooltip": "~1.0.3",
    "@rn-primitives/types": "~1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "^51.0.24",
    "expo-linking": "~6.3.1",
    "expo-navigation-bar": "~3.0.7",
